<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_farmer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zhkj.sfb.FarmerActivity">
    <include layout="@layout/title"
        android:id="@+id/include" />
    <android.support.v7.widget.RecyclerView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/include"
        android:id="@+id/city_list"
        >
    </android.support.v7.widget.RecyclerView>
    <android.support.v7.widget.RecyclerView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/include"
        android:id="@+id/area_list"
        android:layout_toEndOf="@+id/city_list"
        android:layout_marginLeft="3dp">
    </android.support.v7.widget.RecyclerView>
    <android.support.v7.widget.RecyclerView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/include"
        android:id="@+id/township_list"
        android:layout_toEndOf="@+id/area_list"
        android:layout_marginLeft="3dp">
    </android.support.v7.widget.RecyclerView>
    <android.support.v7.widget.RecyclerView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/include"
        android:id="@+id/village_list"
        android:layout_toEndOf="@+id/township_list"
        android:layout_marginLeft="3dp">
    </android.support.v7.widget.RecyclerView>
</RelativeLayout>
